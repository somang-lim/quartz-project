CREATE TABLE TESTER.TEST_SM_QUARTZ
(
	id NUMBER PRIMARY KEY
	, triggerName VARCHAR(50)
	, triggerGroup VARCHAR(50) NOT NULL
	, cronExpression VARCHAR(50) NOT NULL
	, jobName VARCHAR(50) NOT NULL
	, jobGroup VARCHAR(50) NOT NULL
	, isRunning CHAR(1) DEFAULT 'Y' NOT NULL
	, createTime DATE DEFAULT SYSDATE NOT NULL
	, updateTime DATE
);

CREATE SEQUENCE TESTER.TEST_SM_QUARTZ_SEQ
			 INCREMENT BY 1
			 START WITH 1
			 MINVALUE 1
			 MAXVALUE 9999
			 NOCYCLE
			 NOCACHE
			 NOORDER;